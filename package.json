{
  "name": "@xapi/cmi5",
  "version": "0.2.0",
  "description": "Communicate over the xAPI cmi5 profile using JavaScript.",
  "module": "dist/Cmi5.esm.js",
  "browser": "dist/Cmi5.umd.js",
  "typings": "dist/types/Cmi5.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf ./dist",
    "build:js": "rollup --config",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "lint": "eslint . --ext .ts,.js",
    "test": "jest test/*",
    "test:audit": "npm audit --audit-level=moderate --production",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:types": "tsc --noEmit",
    "test:watch": "jest test/* --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xapijs/cmi5.git"
  },
  "keywords": [
    "xapi",
    "cmi5",
    "profile",
    "typescript"
  ],
  "author": "Christian Cook",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xapijs/cmi5/issues"
  },
  "homepage": "https://github.com/xapijs/cmi5",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/node": "^14.11.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "axios-mock-adapter": "^1.18.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "mockdate": "^3.0.2",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@xapi/xapi": "^0.6.9",
    "axios": "^0.20.0",
    "deepmerge": "^4.2.2"
  }
}
